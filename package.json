{
  "name": "tailwind-plugins",
  "version": "1.0.0",
  "private": false,
  "workspaces": [
    "plugins/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "bump-version": "turbo run bump-version --concurrency=1",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write --cache \"**/*.{js,jsx,ts,tsx,md}\"",
    "prepare": "husky install",
    "release": "turbo run release --concurrency=1",
    "test": "dotenv -- turbo run test"
  },
  "dependencies": {
    "@sliit-foss/automatic-versioning": "1.3.0",
    "dotenv-cli": "7.0.0",
    "jest": "29.4.1",
    "prettier": "2.8.3",
    "rimraf": "4.1.2",
    "shx": "0.3.4",
    "turbo": "1.7.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.5",
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@sliit-foss/eslint-config-internal": "1.1.4",
    "eslint": "8.33.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-config-turbo": "0.0.7",
    "husky": "8.0.3"
  },
 
  "packageManager": "pnpm@8.0.0"
}